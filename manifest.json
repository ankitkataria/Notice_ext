{
	"manifest_version":2,
	"name": "My Cool Extension",
	"version":"0.1",
	"content_scripts":[
		{
			"matches":[
				"<all_urls>"
			],
			"js":["content.js"]
		}

	],
	"browser_action":{
		"default_icon": "bulletin-board.png",
		"default_popup": "popup.html",
		"default_title": "Really?"
	},
	"background":{
		"scripts":["background.js"]
	},
	"permissions":[
		"activeTab",
		"https://ajax.googleapis.com/"
	]
}

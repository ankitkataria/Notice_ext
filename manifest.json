{
	"manifest_version":2,
	"name": "My Cool Extension",
	"version":"0.1",
	"content_scripts":[
		{
			"matches":[
				"<all_urls>"
			],
			"js":["jquery-3.1.1.min.js","content.js"]
		}

	],
	"browser_action":{
		"default_icon": "bulletin-board.png",
		"default_popup": "popup.html"
	},
	"background":{
		"scripts":["jquery-3.1.1.min.js","background.js"]
	},
	"permissions":[
		"activeTab",
		"https://ajax.googleapis.com/",
		"http://localhost/*"
	]
}
